{
  "name": "jq-extra-utils",
  "description": "A bunch of convenient filters and functions to use in your jq scripts.",
  "version": "1.0.0",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheap-glitch/jq-extra-utils.git"
  },
  "bugs": {
    "url": "https://github.com/cheap-glitch/jq-extra-utils/issues"
  },
  "author": "cheap glitch <cheap.glitch@gmail.com> (https://github.com/cheap-glitch)",
  "homepage": "https://github.com/cheap-glitch/jq-extra-utils#readme",
  "keywords": [
    "filters",
    "functions",
    "jq",
    "json",
    "module",
    "utilities"
  ],
  "main": "src/utils.jq",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "fd '^[^.]+(|.sh|.bats)$' --type file --min-depth 2 --print0 | xargs --null -- shellcheck --enable=all --external-sources",
    "lint:fix": "npm run lint -- --format=diff | git apply",
    "test": "bats test",
    "test:watch": "watchexec --watch src --watch test --clear -- npm test"
  },
  "devDependencies": {
    "bats-assert": "^2.0.0"
  }
}
